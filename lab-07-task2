#include<stdio.h>
int main(){
	int i;
	int arr[10];
	int search_num;
	int count=0;
	
	for(i=0;i<10;i++){
		printf("Enter number %d: ",i+1);
		scanf("%d",&arr[i]);
	}
	printf("\nEnter the number to search: \n");
	scanf("%d",&search_num);
	
	for(i=0;i<10;i++){
		if(arr[i]==search_num){
			count++;
		}
	}
	if(count>0){
		printf("\nThe number %d has occured %d times",search_num,count);
	}
	else{
		printf("\nnumber not found");
	}
	
	
	return 0;
}
